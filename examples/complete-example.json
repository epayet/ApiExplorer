                            "type": "int"
                        },
                        {
                            "name": "environment",
                            "description": "The environment used",
                            "type": "string"
                        },
                        {
                            "name": "service",
                            "description": "The service called",
                            "type": "string"
                        },
                        {
                            "name": "method",
                            "description": "The method of the service called",
                            "type": "string"
                        },
                        {
                            "name": "parameters",
                            "description": "The parameters used for the method",
                            "type": "string"
                        },
                        {
                            "name": "state",
                            "description": "The state of the execution",
                            "type": "enum",
                            "fields": [
                                {
                                    "name": "pending",
                                    "description": "The execution is still running, you have to check later"
                                },
                                {
                                    "name": "finished",
                                    "description": "The execution is finished and you have now the result and the raw result"
                                }
                            ]
                        },
                        {
                            "name": "result",
                            "description": "The result (json) of the execution when finished",
                            "type": "string"
