        {
            "name": "Saved Request",
            "description": "This resource represents a saved request (for a particular service and method) that you can reload after.",
            "methods": [
                {
                    "verb": "get",
                    "path": "/savedRequest",
                    "description": "Get all saved requests (can be filtered)",
                    "parameters": [
                        {
                            "name": "service",
                            "description": "The service you want the saved requests",
                            "required": false,
                            "parameterType": "query",
                            "exampleValue": "ProductCatalogFrontService"
                        },
                        {
                            "name": "method",
                            "description": "The method of the service you want the saved requests",
                            "required": false,
                            "parameterType": "query",
                            "exampleValue": "GetProductDetailList"
                        }
                    ],
                    "result": [
                        {
                            "name": "name",
                            "description": "The name of the saved request",
                            "type": "string"
