{
    "apiInfo": {
        "title": "WSMonitor API",
        "description": "This API has the purpose to easily manage and call SOAP or RESTful apis.",
        "url": "http://localhost:8080"
    },
    "resources": [
        {
            "name": "Service",
            "description": "This resource represents a web service (soap or rest) with its information.",
            "methods": [
                {
                    "verb": "get",
                    "path": "/service",
                    "description": "Get all services (array)",
                    "result": [
                        {
                            "name": "name",
                            "type": "string",
                            "description": "The name of the service"
                        },
                        {
                            "name": "type",
                            "type": "string",
                            "description": "The type of the service (soap or rest)"
                        },
                        {
                            "name": "endpoint",
                            "type": "string",
                            "description": "The endpoint of the service"
                        }
                    ]
                },
                {
                    "verb": "get",
                    "path": "/service/{name}",
                    "description": "Get one service (by name)",
                    "parameters": [
                        {
                            "name": "name",
                            "description": "The name of the service",
                            "required": true,
                            "parameterType": "id",
                            "exampleValue": "BrandFrontService"
                        }
                    ],
                    "result": [
                        {
                            "name": "name",
                            "type": "string",
                            "description": "The name of the service"
                        },
                        {
                            "name": "type",
                            "type": "string",
                            "description": "The type of the service (soap or rest)"
                        },
                        {
                            "name": "endpoint",
                            "type": "string",
                            "description": "The endpoint of the service"
                        }
                    ]
                },
                {
                    "verb": "post",
                    "path": "/service",
                    "description": "Add a service",
                    "parameters": [
                        {
                            "name": "name",
                            "description": "The name of the service",
                            "required": true,
                            "parameterType": "body",
                            "exampleValue": "testNewService"
                        },
                        {
                            "name": "type",
                            "description": "The type of the service (soap or rest)",
                            "type": "enum",
                            "values": [
                                "soap",
                                "rest"
                            ],
                            "required": true,
                            "parameterType": "body",
                            "exampleValue": "soap"
                        },
                        {
                            "name": "endpoint",
                            "description": "The endpoint of the service (see /endpoint to have endpoints available)",
                            "required": true,
                            "parameterType": "body",
                            "exampleValue": "front"
                        }
                    ],
                    "result": [
                        {
                            "name": "name",
                            "type": "string",
                            "description": "The name of the service"
                        },
                        {
                            "name": "type",
                            "type": "string",
                            "description": "The type of the service (soap or rest)"
                        },
                        {
                            "name": "endpoint",
                            "type": "string",
                            "description": "The endpoint of the service"
                        }
                    ]
                },
                {
                    "verb": "put",
                    "path": "/service/{name}",
                    "description": "Update a service by name",
                    "parameters": [
                        {
                            "name": "name",
                            "description": "The name of the service",
                            "required": true,
                            "parameterType": "id",
                            "exampleValue": "BrandFrontService"
                        },
                        {
                            "name": "type",
                            "description": "The type of the service (soap or rest)",
                            "type": "enum",
                            "values": [
                                "soap",
                                "rest"
                            ],
                            "required": true,
                            "parameterType": "body",
                            "exampleValue": "soap"
                        },
                        {
                            "name": "endpoint",
                            "description": "The endpoint of the service (see /endpoint to have endpoints available)",
                            "required": true,
                            "parameterType": "body",
                            "exampleValue": "front"
                        }
                    ],
                    "result": [
                        {
                            "name": "name",
                            "type": "string",
                            "description": "The name of the service"
                        },
                        {
                            "name": "type",
                            "type": "string",
                            "description": "The type of the service (soap or rest)"
                        },
                        {
                            "name": "endpoint",
                            "type": "string",
                            "description": "The endpoint of the service"
                        }
                    ]
                },
                {
                    "verb": "delete",
                    "path": "/service/{name}",
                    "description": "Delete a service by name",
                    "parameters": [
                        {
                            "name": "name",
                            "description": "The name of the service",
                            "required": true,
                            "parameterType": "id",
                            "exampleValue": "BrandFrontService"
                        }
                    ]
                }
            ]
        },
        {
            "name": "Service Description",
            "methods": [
                {
                    "verb": "get",
                    "path": "/service/{name}/description",
                    "description": "Get the description of a soap service (methods and types)",
                    "parameters": [
                        {
                            "name": "name",
                            "description": "The name of the service",
                            "required": true,
                            "parameterType": "id",
                            "exampleValue": "BrandFrontService"
                        },
                        {
                            "name": "environment",
                            "description": "The environment to use (see /environment to see environments available)",
                            "required": true,
                            "parameterType": "query",
                            "exampleValue": "dev"
                        }
                    ],
                    "result": [
                        {
                            "name": "methods",
                            "type": "array",
                            "description": "Contains the methods available for the service",
                            "fields": [
                                {
                                    "name": "name",
                                    "description": "The name of the method"
                                },
                                {
                                    "name": "parameters",
                                    "description": "The array of parameters of the method (name and type)"
                                }
                            ]
                        },
                        {
                            "name": "types",
                            "type": "array",
                            "description": "Contains the types definition used for the service",
                            "fields": [
                                {
                                    "name": "name",
                                    "description": "The name of the type"
                                },
                                {
                                    "name": "type",
                                    "description": "class, array, int, string, etc."
                                },
                                {
                                    "name": "values",
                                    "description": "In case of a class, contains the public properties"
                                },
                                {
                                    "name": "singleType",
                                    "description": "In case of an array, contains the name of the type of a single object"
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "name": "Service Parameter Skeleton",
            "methods": [
                {
                    "verb": "get",
                    "path": "/service/{name}/parametersSkeleton",
                    "description": "Get the json skeleton for a soap method",
                    "parameters": [
                        {
                            "name": "name",
                            "description": "The name of the service",
                            "required": true,
                            "parameterType": "id",
                            "exampleValue": "BrandFrontService"
                        },
                        {
                            "name": "environment",
                            "description": "The environment to use (see /environment to see environments available)",
                            "required": true,
                            "parameterType": "query",
                            "exampleValue": "dev"
                        },
                        {
                            "name": "method",
                            "description": "The method where you want an empty parameter skeleton",
                            "required": true,
                            "parameterType": "query",
                            "exampleValue": "GetBrandList"
                        }
                    ]
                }
            ]
        },
        {
            "name": "Service Soap Parameters",
            "methods": [
                {
                    "verb": "post",
                    "path": "/service/{name}/parametersSoap",
                    "description": "Get the soap version of the json based parameters for a method",
                    "parameters": [
                        {
                            "name": "name",
                            "description": "The name of the service",
                            "required": true,
                            "parameterType": "id",
                            "exampleValue": "ProductCatalogFrontService"
                        },
                        {
                            "name": "method",
                            "description": "The method used",
                            "required": true,
                            "parameterType": "body",
                            "exampleValue": "GetProductDetailList"
                        },
                        {
                            "name": "environment",
                            "description": "The environment to use (see /environment to see environments available)",
                            "required": true,
                            "parameterType": "body",
                            "exampleValue": "dev"
                        },
                        {
                            "name": "parameters",
                            "description": "The json based parameters to be converted to SOAP",
                            "required": false,
                            "type": "object",
                            "parameterType": "body",
                            "exampleValue": {
                                "headerMessage": {
                                    "Context": {
                                        "CatalogID": 1,
                                        "SiteID": 100
                                    },
                                    "Localization": {
                                        "Country": "Fr",
                                        "Currency": "Eur",
                                        "Language": "Fr"
                                    }
                                },
                                "productIdList": [
                                    "TU03"
                                ]
                            }
                        }
                    ],
                    "result": [
                        {
                            "name": "envelope",
                            "description": "The SOAP envelope which is the SOAP equivalent of the json based parameters for this method",
                            "type": "string"
                        }
                    ]
                }
            ]
        },
        {
            "name": "Execution",
            "description": "Represents an execution running or finished on the server",
            "methods": [
                {
                    "verb": "get",
                    "path": "/execution",
                    "description": "Get all executions",
                    "result": [
                        {
                            "name": "id",
