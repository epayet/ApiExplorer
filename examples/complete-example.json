                        {
                            "name": "service",
                            "description": "The service corresponding",
                            "type": "string"
                        },
                        {
                            "name": "method",
                            "description": "The method corresponding",
                            "type": "string"
                        },
                        {
                            "name": "parameters",
                            "description": "The parameters saved",
                            "type": "object"
                        }
                    ]
                },
                {
                    "verb": "post",
                    "path": "/savedRequest",
                    "description": "Create a savedRequest",
                    "parameters": [
